{{#*inline "title"}}
Projects
{{/inline}}

{{#*inline "heading"}}
Projects
{{/inline}}

{{#*inline "head"}}
  <link rel="stylesheet" href="/static/styles/projects.css">
{{/inline}}

{{#*inline "content"}}
  {{#each categories}}
    <h4>{{@key}}</h4>
    <article class="{{@key}}">
      {{#each this}}
        <a href="/projects/{{id}}">
          <h2>{{title}}</h2>
          <p>{{description}}</p>
        </a>
      {{/each}}
    </article>
  {{/each}}
{{/inline}}
{{>partials/_base}}